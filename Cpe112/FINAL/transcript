# Loading std.standard
# Loading ieee.std_logic_1164(body)
# Loading work.bigpart(bdf_type)
# Loading work.yourpart(bdf_type)
add wave sim:/bigpart/*
noforce sim:/bigpart/countq1
force -freeze sim:/bigpart/countq0 U 0
force -freeze sim:/bigpart/countq0 1 0, 0 {400 ps} -r 800
force -freeze sim:/bigpart/countq1 1 0, 0 {400 ps} -r 800
force -freeze sim:/bigpart/countq2 1 0, 0 {400 ps} -r 800
run
force -freeze sim:/bigpart/countq0 1 0
noforce sim:/bigpart/countq0
force -freeze sim:/bigpart/countq0 1 400
force -freeze sim:/bigpart/countq1 1 200 -cancel 40
force -freeze sim:/bigpart/countq1 1 200 -cancel 400
force -freeze sim:/bigpart/countq1 1 600 -cancel 800
force -freeze sim:/bigpart/countq2 1 100 -cancel 200
force -freeze sim:/bigpart/countq2 1 300 -cancel 400
force -freeze sim:/bigpart/countq2 1 500 -cancel 600
force -freeze sim:/bigpart/countq2 1 700 -cancel 800
run
force -freeze sim:/bigpart/countq0 1 0, 0 {50 ps} -r 100
run
force -freeze sim:/bigpart/countq0 1 0, 0 {50 ps} -r 100
force -freeze sim:/bigpart/countq1 1 0, 0 {100 ps} -r 200
force -freeze sim:/bigpart/countq2 1 0, 0 {200 ps} -r 400
run
run 800ps
add wave sim:/bigpart/*
clear
# invalid command name "clear"
run 800ps
